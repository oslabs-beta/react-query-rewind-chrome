{
    "manifest_version": 3,
    "name": "RQRewind",
    "version": "0.1",
    "description": "State management DevTools for React Query",
    "devtools_page": "devtools.html",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["http://localhost/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "scripting"
    ],
    "host_permissions": ["http://localhost/*"]
}